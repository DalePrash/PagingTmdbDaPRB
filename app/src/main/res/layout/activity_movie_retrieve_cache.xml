<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".movieroom.ActivityMovieRetrieveCache">

    <Button
        android:id="@+id/buttonGetMovie"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_marginTop="482dp"
        android:text="RetreiveData"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:text="Language"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textmovieLanguage"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginTop="100dp"

        android:layout_marginBottom="10dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/button2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"

        android:layout_width="0dp"
        android:layout_height="70dp"

        android:layout_marginTop="180dp"
        android:gravity="center"
        android:text="Id"
        app:layout_constraintBottom_toTopOf="@+id/button2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView

        android:id="@+id/textmovieId"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginTop="230dp"

        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textmovieLanguage" />

    <TextView
        android:id="@+id/textView5"

        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginTop="290dp"

        android:gravity="center"
        android:text="Adult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textmovieLanguage" />

    <TextView

        android:id="@+id/textmovieAdult"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginTop="340dp"

        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textmovieLanguage" />
<!-- android:checked="@{viewmodel.rememberMe}" -->


</androidx.constraintlayout.widget.ConstraintLayout>